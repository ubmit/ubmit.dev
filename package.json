    "@radix-ui/colors": "^3.0.0",
    "astro": "^5.16.6",
    "clsx": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.14.1",
