---
interface Props {
  size?: number;
  class?: string;
}

const { size = 16, class: className } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  fill="none"
  stroke="currentColor"
  stroke-linecap="round"
  stroke-linejoin="round"
  stroke-width="2"
  viewBox="0 0 24 24"
  class={className}
>
  <path stroke="none" d="M0 0h24v24H0z"></path>
  <path d="M17 7 7 17M8 7h9v9"></path>
</svg>
